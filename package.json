  "scripts": {
    "ng": "ng",
    "serve": "ng serve -o",
    "start" : "ng serve && start gulp",
    "start2": "npm-run-all -p serve",
    "start:styles": "npm-run-all -p serve \"generate-live-editing -- --styles {1}\" --",
    "start:watch-live-editing": "npm-run-all -p serve watch-live-editing",
    "start:styles:watch-live-editing": "npm-run-all -p serve \"watch-live-editing -- --styles {1}\" --",
    "generate-live-editing": "gulp generate-live-editing",
    "watch-live-editing": "gulp watch-live-editing",
    "build": "node --max_old_space_size=2048 node_modules/@angular/cli/bin/ng build --configuration production",
    "build-prod": "npm version patch && node ./replace.build.js && node --max_old_space_size=2048 node_modules/@angular/cli/bin/ng build --configuration production",
    "build2": "npm version patch && node ./replace.build.js && node --max_old_space_size=2048 node_modules/@angular/cli/bin/ng build --configuration production",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
